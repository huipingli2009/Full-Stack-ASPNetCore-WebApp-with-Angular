@using PHO_WebApp.Models;
@model PHO_WebApp.Models.FormSection
@{
    Layout = null;
}

@Html.HiddenFor(m => m.FormSectionId)
@for (var i = 0; i < Model.Sections.Count(); i++)
{
    <input type="submit" name="Submit" formaction="@Url.Action("CloneSection","Survey", new { id=Model.Sections[i].SectionId })" formmethod="post" value="Clone" class="btn btn-primary" />
    @Html.EditorFor(m => m.Sections[i])
}